{
  "manifest_version": 2,
  "name": "Automatic Job Tracker",
  "version": "1",
  "description": "An extension that automatically tracks job status based on your work email, categorizing progress into five categories.",
  "key" : "",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "storage"
  ],
  "optional_permissions": [
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://www.blogger.com/"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],
  "icons": {
    "48": "icon.png"
  }
}
